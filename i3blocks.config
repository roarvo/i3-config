[load]
label= 
command=awk '{print $1,$2,$3}' /proc/loadavg
interval=10
color=#6c71c4


[volume]
label=
#command=/usr/lib/i3blocks/volume
command=~/.i3/volume-usb
interval=2
signal=10
color=#d70a53


[time]
#command-date " %a %m-%d-%y %l:%M:%S %p"
label=
command=date '+%a %d-%m-%y %H:%M:%S'
interval=5
color=#50C878


