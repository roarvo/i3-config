[dhcp]
command=if pgrep dhcpcd >/dev/null 2>&1; then echo "DHCP: yes"; echo "DHCP: yes"; echo "#00ff00"; else echo "DHCP: no"; echo "DHCP: no"; "#ff0000"; fi
interval=10

[vpn]
command=if ps aux | grep -f /var/run/openconnect.pid; then echo "<span color='#00ff00'> VPN: yes</span>"; else echo "<span color='#ff0000'> VPN: no</span>"; fi
markup=pango
interval=10

[ethernet]
label=E
instance=enp3s0
command=echo " $(/usr/lib/i3blocks/down)  $(/usr/lib/i3blocks/up)"
color=#00FF00
interval=10

