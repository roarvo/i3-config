separator_block_width=14

[kernel]
command=echo : jabberwocky $(/usr/bin/uname -or)
interval=once
color=#1793d1

[cpu]
label=Cpu:
command=/usr/lib/i3blocks/cpu_usage
interval=10

[temp]
label=
command=echo "$(sensors coretemp-isa-0000 | awk '/Physical/ {print $4}')"
interval=10
color=#b58900

