[disk-root]
label=:
command=df -h -P -l "${BLOCK_INSTANCE:-/}" | awk '/\// { print $4 }'
instance=/
interval=30
color=#855211


[disk-home]
label=:
command=/usr/lib/i3blocks/disk $HOME
interval=30
color=#859900

